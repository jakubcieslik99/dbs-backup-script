{
  "name": "dbs-backup-script",
  "version": "1.2.2",
  "description": "MEGA Drive databases backup upload script",
  "main": "index.ts",
  "scripts": {
    "upload:mongo": "cross-env DB=mongo ts-node ./scripts/index.ts",
    "upload:mysql": "cross-env DB=mysql ts-node ./scripts/index.ts",
    "upload:postgres": "cross-env DB=postgres ts-node ./scripts/index.ts",
    "lint": "eslint . --ignore-path .gitignore --ext .js,.ts",
